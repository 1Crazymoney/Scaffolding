<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <VersionPrefix>1.0.0</VersionPrefix>
        <BuildNumber Condition="'$(BuildNumber)'==''">$([System.DateTime]::Now.Ticks)</BuildNumber>
        <VersionSuffix>msbuild3-$(BuildNumber)</VersionSuffix>
    </PropertyGroup>
    <PropertyGroup>
        <PackageVersion Condition=" '$(AspNetToolsPackageVersion)' == '' ">$(VersionPrefix)-$(VersionSuffix)</PackageVersion>
    </PropertyGroup>
</Project>